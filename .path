# PATH additions
for another_bin in \
    $(brew --prefix coreutils)/libexec/gnubin \
    $HOME/bin \
    /usr/local/php5/bin \
    $HOME/.rbenv/bin \
    /usr/local/share/npm/bin	
do
    [[ -e $another_bin ]] && export PATH=$another_bin:$PATH
done
unset another_bin
export PATH=/usr/local/bin:/usr/local/sbin:$PATH

if which rbenv > /dev/null; then
	eval "$(rbenv init -)"
	rbenv global 1.9.3-p385
fi